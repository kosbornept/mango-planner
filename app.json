{
  "expo": {
    "name": "Mango",
    "scheme": "mango",
    "slug": "mango",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Mango.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffbd59"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#95E0A1"
      },
      "package": "com.kevinosborne.mango"
    },
    "web": {
      "favicon": "./assets/icons/mango-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "430de68c-696d-47d2-89fd-8fd120567c0a"
      }
    }
  }
}
